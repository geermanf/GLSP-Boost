{
  "name": "angular-io-example",
  "version": "1.0.0",
  "private": true,
  "description": "Example project from an angular.io guide.",
  "scripts": {
    "build": "npm run lint && node-sass src/assets/scss -o src/assets/css && pug ./src/app -P",
    "watch:p": "pug ./src/app -P -w",
    "watch:s": "node-sass src/assets/scss -o src/assets/css -w",
    "watch": "concurrently --kill-others \"npm run start\" \"npm run watch:p\" \"npm run watch:s\"",
    "start": "webpack-dev-server --inline --progress --port 7898",
    "test": "karma start karma.webpack.conf.js",
    "dev": "rimraf build && webpack -w --config config/webpack.dev.js",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "prod": "rimraf dist && webpack --config config/webpack.prod.js --bail"
  },
